{
  "expo": {
    "name": "PawFinder",
    "description": "Paw Finder Dog Adoption and Fostering App",
    "slug": "Paw-Finder",
    "version": "1.0",
    "orientation": "portrait",
    "icon": "./assets/expo-app-icon.png",
    "splash": {
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*",
      "assets/audio/*",
      "assets/screen-icons/*"
    ],
    "ios": {
      "supportsTablet": true,
      "usesAppleSignIn": true,
      "bundleIdentifier": "com.paw.finder",
      "config": {
        "usesNonExemptEncryption": false
     },
     "infoPlist": {
       "UIBackgroundModes": [
        "remote-notification"
       ]
     }
    },
    "android": {
      "package": "com.paw.finder",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/expo-app-icon.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": "./assets/expo-app-icon.png"
    }
  }
}
